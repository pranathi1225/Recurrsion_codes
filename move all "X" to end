public class Main
{
    
	public static void main(String[] args) {
	    
	    String s="axbxxc";
	    int c=0;
	    StringBuilder sb=new StringBuilder();
	    for(int i=0;i<sb.length();i++)
	    {
	        if(s.charAt(i)!='x')
	        {
	            sb.append(s.charAt(i));
	        }
	        else
	        {
	            c++;
	        }
	    }
	    for(int i=0;i<c;i++)
	    {
	    sb.append('x');
	    }
	    System.out.print(sb.toString());
	}
}
